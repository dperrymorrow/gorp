{
  "name": "gorp",
  "version": "1.6.0",
  "description": "interactive git terminal",
  "main": "index.js",
  "bin": {
    "gorp": "bin/index.js"
  },
  "scripts": {
    "test": "ava",
    "test:git": "node test/live-git.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dperrymorrow/gorp.git"
  },
  "keywords": [
    "git",
    "node",
    "interactive"
  ],
  "author": "David Morrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dperrymorrow/gorp/issues"
  },
  "homepage": "https://github.com/dperrymorrow/gorp#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "eslint": "^4.12.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-unicorn": "^3.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.9.1",
    "sinon": "^4.1.3"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        70,
        95
      ],
      "functions": [
        70,
        95
      ],
      "branches": [
        70,
        95
      ],
      "statements": [
        70,
        95
      ]
    },
    "check-coverage": true,
    "statements": 50,
    "branches": 42,
    "functions": 25,
    "lines": 60,
    "include": [
      "lib"
    ],
    "exclude": [
      "node_modules",
      "lib/util/lodash.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "ava": {
    "require": [
      "./test/setup.js"
    ],
    "files": [
      "./test/**/*.spec.js"
    ],
    "failFast": true,
    "serial": true
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "child-process-promise": "^2.2.1",
    "diff2html": "^2.3.2",
    "inquirer": "^4.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.last": "^3.0.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "node-cleanup": "^2.1.2",
    "npm": "^5.6.0",
    "opn": "^5.1.0",
    "tmp": "0.0.33"
  }
}
